{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "AccMan",
  "version": "0.1.0",
  "identifier": "com.tauri.dev",
  "build": {
    "frontendDist": "../src/dist",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "pnpm --dir src run dev",
    "beforeBuildCommand": "pnpm --dir src build"
  },
  "app": {
    "windows": [
      {
        "title": "AccMan",
        "width": 1280,
        "height": 900,
        "minWidth": 1280,
        "minHeight": 900,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
